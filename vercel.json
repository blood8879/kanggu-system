{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd kanggu-contract && npm run build",
  "outputDirectory": "kanggu-contract/dist",
  "installCommand": "cd kanggu-contract && npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
